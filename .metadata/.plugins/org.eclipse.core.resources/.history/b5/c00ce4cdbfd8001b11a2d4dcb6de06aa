package com.andrei;

public class For {
	
	public void ejercicioPares100() {
		for(int i=0; i <= 100; i=i+2) {
			/*
			if(i % 2 == 0) {
				System.out.println(i);
			}
			*/
			System.out.println(i);
		}
		
		for(int i=0; i <= 100; i++) {
			/*
			if(i % 2 == 0) {
				System.out.println(i);
			}
			*/
			System.out.println(i);
		}
	}

}
